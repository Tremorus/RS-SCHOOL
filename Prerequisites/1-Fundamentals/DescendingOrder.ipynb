    "def descending_order(num):\n",
    "    a = [s for s in str(num)]\n",
    "    a.sort(reverse=True)\n",
    "    return(int(\"\".join(a)))\n",
    "\n",
    "\n",
    "n='10651'\n",
    "print(n)\n",
    "res = descending_order(n)\n",
    "print(res)\n"
